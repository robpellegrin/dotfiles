# Create keys: ssh-keygen -t rsa -C "your_email@example.com" -f ~/.ssh/id_rsa_account1
# Add keys: ssh-add ~/.ssh/id_rsa_account1


# Account 1
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa_account1

# Account 2
Host github.com-su
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa_account2
